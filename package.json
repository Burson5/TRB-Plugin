{
  "name": "trb-plugin",
  "version": "0.0.1",
  "description": "typeScript+react+antd+mobx+webpack chrome插件脚手架",
  "scripts": {
    "lint": "node lint.js",
    "lint:fix": "npm run lint --fix",
    "clean:lib": "rimraf ./lib",
    "clean:dist": "rimraf ./dist",
    "lib": "npm run clean:lib && webpack --progress --config ./webpack/webpack.lib.conf.js -p",
    "build:anal": "npm run clean:dist && webpack --anal=1",
    "local": "npm run clean:dist && webpack  --hot=true --watch --hide-modules --env=local --uselocal=1",
    "build": "npm run clean:dist && webpack  --env=production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(js|jsx|ts|tsx)": [
      "eslint",
      "git add"
    ],
    "src/**/*.(css|scss|less)": [
      "stylelint",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/chrome": "^0.0.86",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-router": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.3.5",
    "css-loader": "^3.0.0",
    "editorconfig": "^0.15.3",
    "eslint": "^6.4.0",
    "eslint-config-alloy": "^3.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "json-loader": "^0.5.7",
    "jszip": "^3.2.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.5",
    "node-sass": "4.9.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.0",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/runtime-corejs3": "^7.6.3",
    "antd": "^3.24.3",
    "axios": "^0.19.0",
    "crypto-js": "^3.1.9-1",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-intl-universal": "^2.2.1",
    "react-router-dom": "^5.1.2"
  }
}
